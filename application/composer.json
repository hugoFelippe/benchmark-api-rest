{
    "name": "hugofelippe/apirest",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Hugo Felippe de Souza Cruz",
            "email": "hfelipecruz07@gmail.com"
        }
    ],
    "config": {
        "platform":{
            "php":"7.4"
        }
    },
    "require": {
        "lcobucci/jwt": "^4.1",
        "mobiledetect/mobiledetectlib": "^2.8",
        "monolog/monolog": "^2.3",
        "php-amqplib/php-amqplib": "^3.0",
        "php-di/php-di": "^6.3",
        "ralouphie/mimey": "^2.1",
        "slim/psr7": "^1.4",
        "slim/slim": "^4.8",
        "vlucas/valitron": "^1.4",
        "php-pm/php-pm": "^2.2",
        "vlucas/phpdotenv": "^5.3",
        "react/mysql": "^0.5.5"
    },
    "require-dev": {
        "pestphp/pest": "^1.18"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Events\\": "events/",
            "Lib\\": "lib/",
            "Middleware\\": "middleware/",
            "Routes\\": "routes/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "serve": "php -S 0.0.0.0:8000 public/server.php -t ./"
    }
}
